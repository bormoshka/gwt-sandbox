<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='sandbox'>

    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
    <inherits name="org.fusesource.restygwt.RestyGWT"/>
    <inherits name="com.google.gwt.i18n.I18N"/>
    <inherits name='com.google.gwt.http.HTTP'/>
    <inherits name='com.google.gwt.json.JSON'/>
    <!--<inherits name="com.sencha.gxt.widget.core.Core"/>-->
    <!--<inherits name="com.sencha.gxt.theme.triton.Theme"/>-->

    <source path="client"/>

    <entry-point class='ru.ulmc.gwt.sandbox.client.SimpleEntryPoint'/>

    <inherits name='com.google.gwt.core.Core'/>
    <inherits name='com.google.gwt.user.User'/>
    <extend-property name="locale" values="ru"/>
    <stylesheet src="../html/style.css"/>

    <!-- <set-property name="gwt.suppressNonStaticFinalFieldWarnings" value="false"/>-->
    <set-property name="user.agent" value="safari"/>
   <!-- <collapse-all-properties/>-->

    <generate-with class="ru.ulmc.gwt.sandbox.server.rebind.SandGenerator">
        <when-type-assignable class="ru.ulmc.gwt.sandbox.client.api.ServiceAsync"/>
    </generate-with>
</module>